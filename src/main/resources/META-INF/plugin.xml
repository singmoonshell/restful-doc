<idea-plugin>
    <id>com.tiger.restful-doc</id>
    <name>restful-doc</name>
    <vendor email="wangheng@itiger.com" url="https://www.tiger.com">Tiger Broker</vendor>

    <description><![CDATA[
    Restful-doc.<br>
    <em>Restful-doc</em>
    ]]></description>

    <!-- please see https://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.platform</depends>
    <depends>com.intellij.java</depends>

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
    </extensions>

    <actions>
        <!-- Add your actions here -->
        <action id="com.tiger.action.RestfulDocAction" class="com.tiger.action.RestfulDocAction" text="Restful Doc" description="Restful document generator">
            <add-to-group group-id="EditorPopupMenu" anchor="first"/>
        </action>
    </actions>
</idea-plugin>